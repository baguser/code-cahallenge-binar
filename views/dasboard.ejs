    />
    <!-- owl carousel -->
    <link rel="stylesheet" href="css/owl.carousel.css" type="text/css" />
    <link href="css/jquery-jvectormap-1.2.2.css" rel="stylesheet" />
    <!-- Custom styles -->
    <link rel="stylesheet" href="libraries/bootstrap/css/fullcalendar.css" />
    <link href="libraries/bootstrap/css/widgets.css" rel="stylesheet" />
